					<!-- Write out date: Day Month Year -->
					<!-- @when = YYYY-MM-DD -->
                    <date type="publication" when=""></date>
					<!-- Focus region or composition place (if known; otherwise remove) -->
                    <placeName type=""></placeName><!-- type is either focusRegion or compositionPlace -->
