	                    <author></author>
						<editor></editor><!-- Editor (if known; otherwise leave blank -->
						<imprint>
							<biblScope unit="vol"></biblScope><!-- Volume (if known; otherwise leave blank) -->
							<biblScope unit="issue"></biblScope><!-- Issue (if known; otherwise leave blank) -->
							<biblScope unit="pages"></biblScope><!-- Pages (if known; otherwise leave blank) -->
							<pubPlace></pubPlace><!-- Publication place (if known; otherwise leave blank) -->
							<publisher></publisher><!-- Publisher (if known; otherwise leave blank) -->
							<date></date><!-- Item date; same as date in <title type="alternative"> above -->
						</imprint>
					</monogr>
				</biblStruct>
                <bibl type="sourceMetadata">
                	<!-- Title without date -->
					<!-- Same as <title> above -->
					<title></title>
					<!-- Each author: Surname, Forename, Dates (if known; otherwise nothing) -->
					<!-- Same as <author> above -->
					<!-- xml:id = each author's initials -->
                    <author xml:id=""></author>
