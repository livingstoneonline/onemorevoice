                    <placeName type="compositionPlace"></placeName>
                </bibl>
            </sourceDesc>
			<!-- End of manuscript <sourceDesc> -->

			<!-- Use this version of <sourceDesc> if published text; otherwise delete it -->
            <sourceDesc>
				<biblStruct type="journal">
